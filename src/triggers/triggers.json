[
    {
        "name": "Prompt",
        "multiline": "yes",
        "multilineDelta": "0",
        "patterns": [
            {
                "pattern": "[Pl: ", 
                "type": "substring"
            },
            {
                "pattern": "^\\[Pl: (?<PL>[0-9,]+) \\| (?<Metrics>[^]]+)]\\s?(?<Forms>.+)?$",
                "type": "regex"
            }
        ]

    },
    {
        "name": "Prompt Kaioken",
        "multiline": "yes",
        "multilineDelta": "0",
        "patterns": [
            {
                "pattern": "[Kaioken: ", 
                "type": "substring"
            },
            {
                "pattern": "^\\[Kaioken: (?<CURR>\\d+)/(?<MAX>\\d+) ]",
                "type": "regex"
            }
        ]

    },
    {
        "name": "Tell Rpc",
        "multiline": "yes",
        "multilineDelta": "0",
        "patterns": [
            {
                "pattern": "tells you, ", 
                "type": "substring"
            },
            {
                "pattern": "^(\\w+) tells you, '(.+)'$",
                "type": "regex"
            }
        ]
    },
    {
        "name": "MOTD Bypass",
        "patterns": [
               {
                      "pattern": "*** PRESS RETURN: ",
                      "type": "startOfLine"
               },
               {
                      "pattern": "*** PRESS RETURN: ",
                      "type": "exactMatch"
               }
        ],
        "script": "send(\"gogogo\\n\") send(\"\\n\\n\") send(\"status\") send(\"score\") send(\"learn\")"
    }
]
