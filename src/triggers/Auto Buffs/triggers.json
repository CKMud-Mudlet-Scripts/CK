[
       {
              "name": "Status Affections",
              "multiline": "yes",
              "multilineDelta": "1",
              "fireLength": "50",
              "patterns": [
                     {
                            "pattern": "o-----------------------------------------------------------o",
                            "type": "exactMatch"
                     },
                     {
                            "pattern": " Affections:",
                            "type": "exactMatch"
                     }
              ],
              "script": "CK.PromptFlags.affects = {}\nCK.API.Times.reset(\"status\")",
              "children": [
                     {
                            "name": "Active Buff Trigger",
                            "multiline": "yes",
                            "multilineDelta": "0",
                            "patterns": [
                                   {
                                          "pattern": ": In affect for",
                                          "type": "substring"
                                   },
                                   {
                                          "pattern": "\\s*(?\u003caffect\u003e.+)\\s*: In affect for (?\u003ctime\u003e\\d+) mins\\.$",
                                          "type": "regex"
                                   }
                            ]
                     },
                     {
                            "name": "close Affections",
                            "multiline": "yes",
                            "multilineDelta": "1",
                            "patterns": [
                                   {
                                          "pattern": "o-----------------------------------------------------------o",
                                          "type": "exactMatch"
                                   },
                                   {
                                          "pattern": " Equipment Boosts:",
                                          "type": "exactMatch"
                                   }
                            ],
                            "script": "setTriggerStayOpen(\"Status Affections\", 0)"
                     }
              ]
       },

